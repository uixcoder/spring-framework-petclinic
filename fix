find . -type f -name "*" -exec sed -i 's/3.3.6/3.4.1/g' {} +

find . -type f -name "pom.xml" -exec sed -i 's/jdbc:mysql:\/\/localhost:3306/jdbc:mysql:\/\/10.1.1.20:3306/g' {} +
find . -type f -name "pom.xml" -exec sed -i 's/            <id>H2<\/id>
            <activation>
                <activeByDefault>true<\/activeByDefault>
            <\/activation>/            <id>H2<\/id>
            /g' {} +


